<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.teamnova.ej.realreview.activity._DemoTest"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <Button

        android:id="@+id/demoTestDialogue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Dialogue"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button

        android:id="@+id/fregmentTest"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="fregment Test"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/demoTestDialogue"
        app:layout_constraintTop_toBottomOf="@+id/demoTestDialogue"
        app:layout_constraintLeft_toLeftOf="@+id/demoTestDialogue"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/bottomNavigation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="bottom navigation"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/fregmentTest"
        app:layout_constraintTop_toBottomOf="@+id/fregmentTest"
        app:layout_constraintLeft_toLeftOf="@+id/fregmentTest"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/facebookKey"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="facebook hash key"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/bottomNavigation"
        app:layout_constraintTop_toBottomOf="@+id/bottomNavigation"
        app:layout_constraintLeft_toLeftOf="@+id/bottomNavigation"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/glideTest"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="server image Load Test"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/facebookKey"
        app:layout_constraintRight_toRightOf="@+id/facebookKey"
        app:layout_constraintTop_toBottomOf="@+id/bottomNavigation"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/shopDetailTest"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:text="shop Detail Test"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/facebookKey"
        app:layout_constraintRight_toRightOf="@+id/facebookKey"
        app:layout_constraintTop_toBottomOf="@+id/bottomNavigation"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/materialDialogTest"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="144dp"
        android:text="shop Detail Test"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/facebookKey"
        app:layout_constraintRight_toRightOf="@+id/facebookKey"
        app:layout_constraintTop_toBottomOf="@+id/bottomNavigation"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/mainActivityTest"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Main Activity Test"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="8dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="459dp" />


</android.support.constraint.ConstraintLayout>
